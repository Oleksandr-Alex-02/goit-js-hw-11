{{#each this}}

  <div class="gallery_card">
      <a href="{{largeImageURL}}">
        <img class="gallery_img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
      </a>
    <div class="gallery_info">
      <p class="gallery_info-item">
        <b>&#128077; {{likes}}</b>
      </p>
      <p class="gallery_info-item">
        <b>&#128064; {{views}}</b>
      </p>
      <p class="gallery_info-item">
        <b>&#128226; {{comments}}</b>
      </p>
      <p class="gallery_info-item">
        <b>&#128260; {{downloads}}</b>
      </p>
    </div>
  </div>
{{/each}}